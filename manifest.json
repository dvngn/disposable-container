{
  "manifest_version": 2,
  "name": "Disposable Container",
  "version": "1.0.2",
  "permissions": [
    "tabs",
    "cookies",
    "contextualIdentities",
    "contextMenus",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "16": "fingerprint-dark.svg",
      "32": "fingerprint-dark.svg"
    },
    "theme_icons": [
      {
        "light": "fingerprint-light.svg",
        "dark": "fingerprint-dark.svg",
        "size": 16
      },
      {
        "light": "fingerprint-light.svg",
        "dark": "fingerprint-dark.svg",
        "size": 32
      }
    ]
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
